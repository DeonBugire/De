// Создать список из 5 любых строк. Вывести все элементы этого списка на печать через .forEach
// Создать достаточно большой список из любых чисел. Найти сумму всех нечетных чисел в списке.


fun main() {
    val rainbow = listOf("Каждый", "Охотник", "Желает", "Знать", "Где")
    rainbow.forEach { println(it) }
    val numbers = mutableListOf(1, 2, 5, 6, 100, 101, 40000, 103, 55)
    numbers.removeIf { it % 2 == 0 }
    println(numbers.sumOf { it })
}
